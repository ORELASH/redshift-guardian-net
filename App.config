<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <!-- Local metadata database (SQL Server Compact or SQLite) -->
    <add name="LocalDB"
         connectionString="Data Source=|DataDirectory|\RedshiftGuardian.sdf;Persist Security Info=False;"
         providerName="System.Data.SqlServerCe.4.0" />
  </connectionStrings>

  <appSettings>
    <!-- Redshift ODBC Driver Settings -->
    <add key="RedshiftOdbcDriver" value="Amazon Redshift (x64)" />
    <add key="DefaultAwsProfile" value="default" />
    <add key="DefaultRegion" value="us-east-1" />
    <add key="DefaultPort" value="5439" />

    <!-- Application Settings -->
    <add key="EnableIAMByDefault" value="true" />
    <add key="EnableSSL" value="true" />
  </appSettings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client" />
  </startup>
</configuration>
